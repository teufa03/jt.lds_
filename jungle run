<script>
    var consent = {
        age: "",
        cmds: [],
        cmd: function(cb){
            consent.cmds.push(cb);
        },

        init: function()
        {
            window.__cmp("addEventListener", ["init", this.cmp_init, false], null)
            window.__cmp("addEventListener", ["settings", this.cmp_settings, false], null)
            window.__cmp("addEventListener", ["consentscreen", this.cmp_consentscreen, false], null)
            window.__cmp("addEventListener", ["consentscreenoff", this.cmp_consentscreenoff, false], null)
            window.__cmp("addEventListener", ["consent", this.cmp_consent, false], null)
            window.__cmp("addEventListener", ["consentapproved", this.cmp_consentapproved, false], null)
            window.__cmp("addEventListener", ["consentrejected", this.cmp_consentrejected, false], null)
            window.__cmp("addEventListener", ["consentcustom", this.cmp_consentcustom, false], null)
            window.__cmp("addEventListener", ["settings", this.cmp_settings, false], null)
            window.__cmp('setAgeCallback', this.ageCallback);
        },

        ageCallback: function(y,m,d,a){
            if (y==0){
                /*
                if year is not set, then we mark it as "undetermined", meaning the cmp flashes
                the year-selection red.
                 */
                return 2;
            }

            /*
            * Legal age of consent, around the globe, highest value, 18
            * */
            if (a>=18){
                consent.age = "ok";
                return 1;
            }
            
            consent.age = "notok";
            return 0;
        },

        cmp_init: function(e, o) { },
        cmp_settings: function(e,o) { },
        cmp_consentscreen: function(e,o) { },
        cmp_consentscreenoff: function(e,o) { },
        cmp_consent: function(e,o) {
            consent.the_consent = o;
            consent.standardPurposeObject = o.getPublisherConsents().standardPurposeConsents;
            consent.runCmd();
            
            if (consent.age == "notok"){
                window.location.href = "https://kids.kiloo.com";
                return 1;
            }
        },

        cmp_consentapproved: function(e,o){
            window.location.reload();
        },

        cmp_consentrejected: function(e,o){
            window.location.reload();
        },

        cmp_consentcustom: function(e,o){
            window.location.reload();
        },

        runCmd: function(){
            consent.cmd = consent.runSingleCmd;
            for(i=0;i<consent.cmds.length;i++)
            {
                consent.runSingleCmd( consent.cmds[i] );
            }
            consent.cmds = [];
        },
        runSingleCmd: function(cb){ cb(); },

        show: function(){
            window.__cmp('showScreen');
        },

        canAds: function(){
            return consent.standardPurposeObject[3] && consent.standardPurposeObject[1] && consent.standardPurposeObject[2];
        },
        canAnalytics: function(){
            return consent.standardPurposeObject[4] && consent.standardPurposeObject[5];
        },

    };
    consent.init();

</script>




    
    
